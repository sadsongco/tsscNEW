<section class="blogPreviews">
<h2>More blogs...</h2>
{{#other_blogs}}
    <article class="otherBlog" hx-get="php/blog.php" hx-vals='{"blog_id":"{{id}}"}' hx-target="#blogContainer" hx-swap="innerHTML show:window:top" hx-push-url="?blog_id={{id}}">
        <h2>{{title}}</h2>
        <p>{{{content}}}</p>
    </article>
{{/other_blogs}}
</section>